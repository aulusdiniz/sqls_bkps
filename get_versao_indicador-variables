SELECT id_indicador, id_versao_indicador_variavel, nm_letra, versao_indicador_variavel_medicao.nr_medicao, nr_periodo, nm_versao_indicador 
FROM versao_indicador
INNER JOIN versao_indicador_variavel USING (id_versao_indicador)
INNER JOIN versao_indicador_variavel_medicao USING (id_versao_indicador_variavel)
--WHERE id_indicador = 134 AND id_versao_indicador_variavel IN (201, 202)
WHERE id_indicador > 133 AND id_indicador < 140 AND id_versao_indicador_variavel > 200 AND id_versao_indicador_variavel < 213
ORDER BY id_indicador, nm_letra, nr_periodo DESC;
